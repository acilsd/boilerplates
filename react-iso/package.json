{
  "name": "react-iso",
  "version": "0.0.2",
  "description": "react-express isomorphic app",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev-api\" \"npm run dev-server\" \"npm run webpack-dev-server\"",
    "dev-api": "set NODE_ENV=development && nodemon --exec babel-node ./server/api.js",
    "dev-server": "set NODE_ENV=development && nodemon --exec babel-node ./server/server.js",
    "webpack-dev-server": "set NODE_ENV=development && babel-node ./server/webpack-dev-server.js",
    "start": "concurrently --kill-others \"npm run start-api\" \"npm run start-server\"",
    "start-api": "set NODE_ENV=production && babel-node ./server/api.js",
    "start-server": "set NODE_ENV=production && babel-node ./server/server.js"
  },
  "author": "acilsd",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "file-loader": "^0.9.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
